        <skos:altLabel xml:lang="nl">Rollator</skos:altLabel>
        <skos:exactMatch>http://purl.bioontology.org/ontology/SNOMEDCT/404946006</skos:exactMatch>
    </owl:Class>
    


    <!-- http://www.zinl.nl/ontologies/VPH-domain-ontology#WholesaleOrganization -->

    <owl:Class rdf:about="http://www.zinl.nl/ontologies/VPH-domain-ontology#WholesaleOrganization">
        <rdfs:subClassOf rdf:resource="http://www.zinl.nl/ontologies/VPH-domain-ontology#MercantileOrganization"/>
        <skos:altLabel xml:lang="nl">GroothandelOrganisatie</skos:altLabel>
        <skos:definition xml:lang="nl">Editorial: Groothandel is een Handelsonderneming die FysiekeObjecten verkoopt aan Organisaties en niet aan individuele Personen.</skos:definition>
        <skos:definition xml:lang="en">Editorial: WholesaleOrganization is a MercantileOrganization that sells PhysicalObjects to Organizations and not to individual Persons.</skos:definition>
        <skos:definition xml:lang="en">http://www.ontologyportal.org/translations/SUMO.owl.txt#WholesaleStore</skos:definition>
        <vph:thesaurus xml:lang="nl">&apos;groothandel&apos;</vph:thesaurus>
    </owl:Class>
    


    <!-- http://www.zinl.nl/ontologies/VPH-domain-ontology#WorkCouncil -->

    <owl:Class rdf:about="http://www.zinl.nl/ontologies/VPH-domain-ontology#WorkCouncil">
        <rdfs:subClassOf rdf:resource="http://www.zinl.nl/ontologies/VPH-domain-ontology#CoDeterminationOrganization"/>
        <skos:altLabel xml:lang="nl">Ondernemingsraad</skos:altLabel>
        <skos:definition xml:lang="nl">Editorial: Ondernemingsraad is een Medezeggenschapsorganisatie die bestaat uit Werknemers van een Organisatie die namens de overige Werknemers overleg voeren met Personen die de leiding hebben over de Organisatie over het Beleid en de belangen van de Werknemers.</skos:definition>
        <skos:definition xml:lang="en">Editorial: WorkCounsil is a CoDeterminationOrganization in which Employees of some Organization represent the interests of all Employees and discuss topics regarding management and policies proposed by the management team of the Organization.</skos:definition>
    </owl:Class>
    


    <!-- http://www.zinl.nl/ontologies/VPH-domain-ontology#Work_Address_Description -->

    <owl:Class rdf:about="http://www.zinl.nl/ontologies/VPH-domain-ontology#Work_Address_Description">
        <rdfs:subClassOf rdf:resource="http://www.zinl.nl/ontologies/VPH-domain-ontology#Address_Description"/>
        <editorNote>TO DO</editorNote>
        <editorNote xml:lang="nl">Werkadres / Optioneel</editorNote>
    </owl:Class>
    


    <!-- http://www.zinl.nl/ontologies/VPH-domain-ontology#Worker -->

    <owl:Class rdf:about="http://www.zinl.nl/ontologies/VPH-domain-ontology#Worker">
        <rdfs:subClassOf rdf:resource="http://www.zinl.nl/ontologies/VPH-domain-ontology#RelationalRole"/>
        <skos:altLabel xml:lang="nl">Medewerker</skos:altLabel>
        <skos:altLabel xml:lang="nl">Werker</skos:altLabel>
        <skos:closeMatch>http://purl.bioontology.org/ontology/SNOMEDCT/106541005</skos:closeMatch>
        <skos:definition xml:lang="en">Editorial: Worker identifies the role of some Human that carries out some task, either acting under orders of someone else and with some financial compensation or out of one&apos;s own free will and without any compensation.</skos:definition>
        <editorNote xml:lang="en">Worker represents the most neutral concept for anyone who carries out some task, irrespective of labour laws and the existence of e.g. an employment contract.</editorNote>
    </owl:Class>
    


    <!-- http://www.zinl.nl/ontologies/VPH-domain-ontology#Wound -->

    <owl:Class rdf:about="http://www.zinl.nl/ontologies/VPH-domain-ontology#Wound">
        <rdfs:subClassOf rdf:resource="http://www.zinl.nl/ontologies/VPH-domain-ontology#Lesion"/>
        <skos:exactMatch xml:lang="en">http://purl.bioontology.org/ontology/SNOMEDCT/13924000</skos:exactMatch>
    </owl:Class>
    


    <!-- http://www.zinl.nl/ontologies/VPH-domain-ontology#WoundCareTherapeuticIntervention -->

    <owl:Class rdf:about="http://www.zinl.nl/ontologies/VPH-domain-ontology#WoundCareTherapeuticIntervention">
        <rdfs:subClassOf rdf:resource="http://www.zinl.nl/ontologies/VPH-domain-ontology#TherapeuticIntervention"/>
        <skos:closeMatch>http://purl.bioontology.org/ontology/SNOMEDCT/225357008</skos:closeMatch>
        <skos:closeMatch>http://purl.bioontology.org/ontology/SNOMEDCT/225358003</skos:closeMatch>
        <skos:definition xml:lang="en">Editorial: WoundCareTherapeuticIntervention is a TherapeuticIntervention carried out with the intention to  care for some Wound in such way that the condition of the Wound stabilizes or improves.</skos:definition>
    </owl:Class>
    


    <!-- http://www.zinl.nl/ontologies/VPH-domain-ontology#WoundDebridement -->

    <owl:Class rdf:about="http://www.zinl.nl/ontologies/VPH-domain-ontology#WoundDebridement">
        <rdfs:subClassOf rdf:resource="http://www.zinl.nl/ontologies/VPH-domain-ontology#WoundCareTherapeuticIntervention"/>
        <skos:definition xml:lang="en">Editorial: WoundDebridement is a WoundCareTherapeuticIntervention where infected and/or necrotic tissue is removed with the intention to stabilize or improve the condition of some Wound.</skos:definition>
        <skos:exactMatch>http://purl.bioontology.org/ontology/SNOMEDCT/85875009</skos:exactMatch>
    </owl:Class>
    


    <!-- http://www.zinl.nl/ontologies/VPH-domain-ontology#WoundHealingProcess -->

    <owl:Class rdf:about="http://www.zinl.nl/ontologies/VPH-domain-ontology#WoundHealingProcess">
        <rdfs:subClassOf rdf:resource="http://www.zinl.nl/ontologies/VPH-domain-ontology#AutonomousHealingProcess"/>
        <skos:closeMatch>http://purl.bioontology.org/ontology/SNOMEDCT/162866007</skos:closeMatch>
        <skos:definition xml:lang="en">Editorial: WoundHealingProcess is an AutonomousHealingProcess where the biological functions in some Organism autonomously regenerate the tissue that was damaged or lost due to some DiseaseOrSyndrome, some InjuringEvent or some TherapeuticIntervention.</skos:definition>
    </owl:Class>
    


    <!-- http://www.zinl.nl/ontologies/VPH-domain-ontology#ZeroHoursEmploymentContract -->

    <owl:Class rdf:about="http://www.zinl.nl/ontologies/VPH-domain-ontology#ZeroHoursEmploymentContract">
        <rdfs:subClassOf rdf:resource="http://www.zinl.nl/ontologies/VPH-domain-ontology#EmploymentContract"/>
        <skos:altLabel xml:lang="nl">NulUrenContract</skos:altLabel>
        <editorNote xml:lang="en">Also see (Dutch only): https://www.rijksoverheid.nl/onderwerpen/arbeidsovereenkomst-en-cao/vraag-en-antwoord/welke-contracten-zijn-er-voor-oproepkrachten</editorNote>
        <editorNote xml:lang="nl">Editorial: NulUrenArbeidscontract is een Arbeidscontract waarin uitdrukkelijk geen afspraken zijn vastgelegd over het aantal uren dat een Werknemer oproepbaar is voor het verrichten van werkzaamheden.</editorNote>
    </owl:Class>
    


    <!-- https://www.w3.org/TR/owl-time/#time:Duration -->

    <owl:Class rdf:about="https://www.w3.org/TR/owl-time/#time:Duration">
        <rdfs:subClassOf rdf:resource="http://www.zinl.nl/ontologies/VPH-domain-ontology#TemporalValue"/>
        <rdfs:isDefinedBy>http://www.w3.org/2006/time#Duration</rdfs:isDefinedBy>
        <skos:definition xml:lang="en">time:Duration of a temporal extent expressed as a decimal number scaled by a temporal unit.</skos:definition>
        <editorNote xml:lang="en">Alternative to time:DurationDescription to support description of a temporal duration other than using a calendar/clock system.</editorNote>
    </owl:Class>
    


    <!-- https://www.w3.org/TR/owl-time/#time:DurationDescription -->

    <owl:Class rdf:about="https://www.w3.org/TR/owl-time/#time:DurationDescription">
        <rdfs:subClassOf rdf:resource="http://www.zinl.nl/ontologies/VPH-domain-ontology#TemporalValue"/>
        <rdfs:isDefinedBy>http://www.w3.org/2006/time#DurationDescription</rdfs:isDefinedBy>
        <skos:definition xml:lang="en">time: Description of temporal extent structured with separate values for the various elements of a calendar-clock system. The temporal reference system is fixed to Gregorian Calendar, and the range of each of the numeric properties is restricted to xsd:decimal.</skos:definition>
    </owl:Class>
    


    <!-- https://www.w3.org/TR/owl-time/#time:Instant -->

    <owl:Class rdf:about="https://www.w3.org/TR/owl-time/#time:Instant">
        <rdfs:subClassOf rdf:resource="http://www.zinl.nl/ontologies/VPH-domain-ontology#TemporalValue"/>
        <rdfs:isDefinedBy>http://www.w3.org/2006/time#Instant</rdfs:isDefinedBy>
        <skos:closeMatch xml:lang="en">http://purl.org/nemo/gufo#Instant</skos:closeMatch>
        <skos:definition>time: A temporal entity with zero extent or duration.</skos:definition>
    </owl:Class>
    


    <!-- https://www.w3.org/TR/owl-time/#time:Interval -->

    <owl:Class rdf:about="https://www.w3.org/TR/owl-time/#time:Interval">
        <rdfs:subClassOf rdf:resource="http://www.zinl.nl/ontologies/VPH-domain-ontology#TemporalValue"/>
        <rdfs:isDefinedBy>http://www.w3.org/2006/time#Interval</rdfs:isDefinedBy>
        <skos:definition>time: A temporal entity with an extent or duration.</skos:definition>
    </owl:Class>
    


    <!-- https://www.w3.org/TR/owl-time/#time:ProperInterval -->

    <owl:Class rdf:about="https://www.w3.org/TR/owl-time/#time:ProperInterval">
        <rdfs:subClassOf rdf:resource="https://www.w3.org/TR/owl-time/#time:Interval"/>
        <rdfs:isDefinedBy>http://www.w3.org/2006/time#ProperInterval</rdfs:isDefinedBy>
        <skos:definition xml:lang="en">time: A temporal entity with non-zero extent or duration, i.e. for which the value of the beginning and end are different.</skos:definition>
    </owl:Class>
    


