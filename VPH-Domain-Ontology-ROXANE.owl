        <skos:exactMatch xml:lang="en">http://bag.basisregistraties.overheid.nl/def/bag#Pand</skos:exactMatch>
        <skos:exactMatch>http://www.ontologyportal.org/translations/SUMO.owl.txt#Building</skos:exactMatch>
        <vph:thesaurus xml:lang="nl">&apos;gebouw&apos;, &apos;pand&apos;,</vph:thesaurus>
    </owl:Class>
    


