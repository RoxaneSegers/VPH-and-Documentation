    <owl:Class rdf:about="http://www.zinl.nl/ontologies/VPH-domain-ontology#KVK_Identifier">
        <rdfs:subClassOf rdf:resource="http://www.zinl.nl/ontologies/VPH-domain-ontology#FormalIdentifierDescription"/>
    </owl:Class>
    


    <!-- http://www.zinl.nl/ontologies/VPH-domain-ontology#LandArea -->

    <owl:Class rdf:about="http://www.zinl.nl/ontologies/VPH-domain-ontology#LandArea">
