        <skos:exactMatch>http://www.ontologyportal.org/translations/SUMO.owl.txt#LandTransitWay</skos:exactMatch>
    </owl:Class>
    


    <!-- http://www.zinl.nl/ontologies/VPH-domain-ontology#Law -->

    <owl:Class rdf:about="http://www.zinl.nl/ontologies/VPH-domain-ontology#Law">
        <rdfs:subClassOf rdf:resource="http://www.zinl.nl/ontologies/VPH-domain-ontology#SocialDescription"/>
        <skos:closeMatch xml:lang="en">http://www.ontologyportal.org/translations/SUMO.owl.txt#Law</skos:closeMatch>
        <skos:definition xml:lang="en">Editorial: A law is a description imposed by some authority that sets forth a rule governing some particular activity and that can be enforced with penalties for noncompliance.</skos:definition>
    </owl:Class>
    


